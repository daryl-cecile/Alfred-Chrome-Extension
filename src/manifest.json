{
    "name": "Alfred",
    "description": "Keep your tab game under control",
    "version": "1.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage", "tabs", "alarms"],
    "options_page": "ui/options/index.html",
    "action": {
        "default_popup": "ui/popup/index.html",
        "default_icon": {
            "128": "images/albert_icon.png"
       }
    },
    "icons": {
        "128": "images/albert_icon.png"
    }
}